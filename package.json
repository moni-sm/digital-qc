{
  "name": "digital-qc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run digital-qc:serve-ssr",
    "serve:ssr": "node dist/digital-qc/server/main.js",
    "build:ssr": "ng build && ng run digital-qc:server",
    "clean-start": "node --eval \"require('fs').rmSync('.angular/cache', { recursive: true, force: true, maxRetries: 3, retryDelay: 1000 })\" && ng serve",
    "hard-reset": "taskkill /f /im node.exe & rimraf .angular/cache node_modules package-lock.json & npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.6",
    "@angular/cdk": "17.3.6",
    "@angular/common": "17.3.6",
    "@angular/compiler": "17.3.6",
    "@angular/core": "17.3.6",
    "@angular/forms": "17.3.6",
    "@angular/language-service": "17.3.6",
    "@angular/material": "^17.3.6",
    "@angular/platform-browser": "17.3.6",
    "@angular/platform-browser-dynamic": "17.3.6",
    "@angular/platform-server": "17.3.6",
    "@angular/router": "17.3.6",
    "@angular/ssr": "17.3.6",
    "@babel/runtime": "7.27.0",
    "@ngneat/content-loader": "7.0.0",
    "@ngneat/tailwind": "7.0.3",
    "@ngneat/until-destroy": "10.0.0",
    "@tailwindcss/postcss": "4.1.4",
    "digital-qc": "file:",
    "express": "^4.21.2",
    "rxjs": "7.8.1",
    "tslib": "2.4.1",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.6",
    "@angular/cli": "17.3.6",
    "@angular/compiler-cli": "17.3.6",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.21",
    "hard-reset": "taskkill /f /im node.exe & rimraf .angular/cache node_modules package-lock.json & npm install",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17"
  }
}
